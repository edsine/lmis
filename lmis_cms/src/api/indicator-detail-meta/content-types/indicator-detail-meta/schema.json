{
  "kind": "collectionType",
  "collectionName": "indicator_detail_metas",
  "info": {
    "singularName": "indicator-detail-meta",
    "pluralName": "indicator-detail-metas",
    "displayName": "Indicator Detail Meta",
    "description": ""
  },
  "options": {
    "draftAndPublish": true,
    "populateCreatorFields": true
  },
  "pluginOptions": {},
  "attributes": {
    "indicator_detail": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::indicator-detail.indicator-detail"
    },
    "gender": {
      "type": "enumeration",
      "enum": [
        "Male",
        "Female"
      ]
    },
    "age_group": {
      "type": "enumeration",
      "enum": [
        "youth",
        "prime-age",
        "elderly"
      ]
    },
    "geographical_region": {
      "type": "enumeration",
      "enum": [
        "urban",
        "rural"
      ]
    },
    "employment_status": {
      "type": "enumeration",
      "enum": [
        "employed",
        "unemployed",
        "inactive"
      ]
    },
    "employment_type": {
      "type": "enumeration",
      "enum": [
        "full-time",
        "part-time",
        "contract"
      ]
    },
    "sector": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::sector.sector"
    },
    "occupation": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::occupation.occupation"
    },
    "skill_level": {
      "type": "enumeration",
      "enum": [
        "high-skilled",
        "low-skilled"
      ]
    },
    "duration_of_unemployment": {
      "type": "enumeration",
      "enum": [
        "short-term",
        "long-term"
      ]
    },
    "educational_level": {
      "type": "enumeration",
      "enum": [
        "primary",
        "secondary",
        "vocational",
        "bachelors",
        "masters",
        "doctoral"
      ]
    },
    "reasons_for_inactivity": {
      "type": "enumeration",
      "enum": [
        "discouraged workers",
        "students",
        "homemakers"
      ]
    },
    "migration_flow": {
      "type": "enumeration",
      "enum": [
        "inward",
        "outward"
      ]
    },
    "reason_for_migration": {
      "type": "enumeration",
      "enum": [
        "economic",
        "political",
        "social"
      ]
    }
  }
}
